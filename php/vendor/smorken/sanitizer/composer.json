{
  "name": "smorken/sanitizer",
  "type": "library",
  "description": "Sanitizer helper",
  "keywords": [
    "laravel",
    "sanitize"
  ],
  "license": [
    "MIT",
    "GPL-2.0+"
  ],
  "authors": [
    {
      "name": "Scott Morken",
      "email": "scott.morken@phoenixcollege.edu"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "5.5.*"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.0",
    "smorken/docker": "*"
  },
  "autoload": {
    "psr-4": {
      "Smorken\\Sanitizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Smorken\\Sanitizer\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "6.x-dev",
      "dev-5.5": "5.5.x-dev",
      "dev-5.4": "5.4.x-dev",
      "dev-5.1": "5.1.x-dev"
    },
    "laravel": {
      "providers": [
        "Smorken\\Sanitizer\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
